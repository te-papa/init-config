{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "no-mixed-operators": 0
  },
  "globals": {
    "fetch": true
  },
  "env": {
    "jest": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "json"
  ]
}
